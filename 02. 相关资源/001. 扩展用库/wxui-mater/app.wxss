/* padding */
.padding-box {
  padding: 0 24rpx;
  box-sizing: border-box;
}

.pl-12 {
  padding-left: 12rpx;
}

.pr-12 {
  padding-right: 12rpx;
}

.p-24 {
  padding: 24rpx;
}

.p-15 {
  padding: 15rpx 24rpx;
}

.p-30 {
  padding: 30rpx;
}

/* margin */
.margin-box {
  margin: 0 24rpx;
}

.ml-12 {
  margin-left: 12rpx;
}

.mr-12 {
  margin-right: 12rpx;
}

.m-24 {
  margin: 24rpx;
}

.m-10 {
  margin: 10rpx;
}

.m-15 {
  margin: 15rpx 24rpx;
}

.m-30 {
  margin: 30rpx;
}

/* 超出换行 */
.onelist-hidden {
  display: block;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}

.twolist-hidden {
  display: -webkit-box;
  word-break: break-all;
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}

/* radius */
.radius-8 {
  border-radius: 8rpx;
  overflow: hidden;
}

.radius-10 {
  border-radius: 10rpx;
  overflow: hidden;
}

.radius-50 {
  border-radius: 500rpx;
  overflow: hidden;
}

/* border */
.b-b {
  border-bottom: 1px solid #eee;
}

.b-m {
  border: 1px solid #DCAB7E;
}

/* 无样式button (用于伪submit) */
.btn-normal {
  display: block;
  margin: 0;
  padding: 0;
  line-height: normal;
  background: none;
  border-radius: 0;
  box-shadow: none;
  border: none;
  /* font-size: unset; */
  text-align: unset;
  overflow: visible;
  /* color: inherit; */
}

.btn-normal:after {
  border: none;
}

.btn-normal.button-hover {
  color: inherit;
}

button:after {
  content: none;
  border: none;
}

/* switch */
.wx-switch-input {
  width: 80rpx !important;
  height: 44rpx !important;
  background: #DCAB7E !important;
  border: 1rpx solid #EEE !important;
}

.wx-switch-input::before {
  width: 77rpx !important;
  height: 40rpx !important;
}

.wx-switch-input::after {
  width: 40rpx !important;
  height: 40rpx !important;
}

.at-switch {
  position: relative;
  margin-right: 43rpx;
  width: 0;
  padding: 0;
}

/* radio */
/* radio未选中时样式 */
radio .wx-radio-input {
  border-radius: 50rpx;
  height: 30rpx;
  width: 30rpx;
  margin: 0;
}

/* radio选中时样式 */
radio .wx-radio-input .wx-radio-input-checked::before {
  color: #DCAB7E;
  background-color: #DCAB7E;
  border: 1rpx solid #DCAB7E;
}

/* checkbox */
checkbox .wx-checkbox-input{
  border-radius: 3rpx;
  height: 30rpx;
  width: 30rpx;
  margin-top: -4rpx;
  border: 1rpx solid #E9C7A2;
}
checkbox .wx-checkbox-input.wx-checkbox-input-checked::before {
width: 32rpx;
height: 32rpx;
line-height: 32rpx;
text-align: center;
font-size: 22rpx;
color: #fff;
background: transparent;
transform: translate(-50%, -50%) scale(1);
-webkit-transform: translate(-50%, -50%) scale(1);
background-color: #E9C7A2;
border: 1rpx solid #E9C7A2;
}

/************************************** rebuild **************************************/
/* 暂无内容 */
.no-content{flex-direction: column;justify-content: center;height: 60vh;}
.no-content span{font-size: 200rpx;display: block;margin-bottom: 20rpx;}

/* 搜索 */
.search-wrap{padding: 0 30rpx;height: 66rpx;}
.search-wrap input{margin-left: 20rpx;height: 66rpx;line-height: 66rpx;background-color: transparent;}
.history-text{justify-content: flex-start;padding: 20rpx 0;}
.history-text .item{background-color: #FBFBFD;padding: 0 30rpx;line-height: 60rpx;color: #242424;}

/* 评价列表 */
.appraise_wrap{margin-top: 20rpx;padding: 0 20rpx;}
.appraise_wrap .tit{height: 95rpx;}
.appraise_wrap .appraise .item{padding: 30rpx 0;align-items: flex-start;}
.appraise_wrap .appraise .item:last-child{border:0;}
.appraise_wrap .appraise .item .logo{width: 80rpx;height: 80rpx;margin-right: 30rpx;}
.appraise_wrap .info_wrap .row .left view:nth-child(1){margin-bottom: 5rpx;}
.appraise_wrap .info_wrap .row .right{justify-content: flex-end;}
.appraise_wrap .info_wrap .row .right text{line-height: 33rpx;padding: 0 8rpx;}
.appraise_wrap .info_wrap .row .right .star{justify-content: flex-start;margin-left: 10rpx;}
.appraise_wrap .info_wrap .row .right .star image{display: block;width: 24rpx;height: auto;margin: 0 0 0 6rpx;}
.appraise_wrap .info_wrap .tag{justify-content: flex-start;}
.appraise_wrap .info_wrap .tag text{font-size: 20rpx;color: #DCAB7E;border: 1px solid rgba(220,171,126,.5);border-radius: 5rpx;padding: 0 8rpx;margin: 15rpx 15rpx 15rpx 0;line-height: 35rpx;}
.appraise_wrap .info_wrap .intro{color: #333;font-size: 30rpx;line-height: 44rpx;padding: 5rpx 0;}
.appraise_wrap .info_wrap .img_wrap{justify-content: flex-start;flex-flow: row wrap;}
.appraise_wrap .info_wrap .img_wrap image{display: block;width: calc(33.3% - 25rpx);margin: 14rpx 25rpx 14rpx 0;height: 160rpx;border-radius: 8rpx;}

